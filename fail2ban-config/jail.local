[DEFAULT]
bantime = 3600
findtime = 300
maxretry = 5
backend = auto
banaction = iptables-multiport
usedns = warn

[squid]
enabled  = true
port     = 3128
filter   = squid
logpath  = /var/log/squid/access.log
maxretry = 5

[webmin-auth]
enabled  = true
port = 10000
filter = webmin-auth
logpath = /var/log/auth.log  ; หรือ /var/webmin/miniserv.log ถ้า Webmin เขียน log ตรงนี้
maxretry = 4
bantime  = 600
